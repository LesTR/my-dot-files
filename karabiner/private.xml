	<item>
		<name>ME: swap ~ and Â±</name>
		<identifier>private.swapTilda</identifier>
		<block>
			<device_only>
				DeviceVendor::APPLE_COMPUTER, DeviceProduct::APPLE_WIRELESS_KEYBOARD_0x0256
			</device_only>
			<autogen>__KeyToKey__ KeyCode::BACKQUOTE,  KeyCode::UK_SECTION</autogen>
			<autogen>__KeyToKey__ KeyCode::UK_SECTION, KeyCode::BACKQUOTE</autogen>
		</block>
	</item>
	<item>
		<name>ME: Fn+hjkl to Left/Down/Up/Right</name>
		<identifier>private.vimode_fn_hjkl_me</identifier>
		<autogen>__KeyToKey__ KeyCode::I, ModifierFlag::FN|ModifierFlag::NONE, KeyCode::HOME</autogen>
		<autogen>__KeyToKey__ KeyCode::A, ModifierFlag::FN|ModifierFlag::NONE, KeyCode::END</autogen>
		<autogen>__KeyToKey__ KeyCode::H, ModifierFlag::FN|ModifierFlag::NONE, KeyCode::CURSOR_LEFT</autogen>
		<autogen>__KeyToKey__ KeyCode::J, ModifierFlag::FN|ModifierFlag::NONE, KeyCode::CURSOR_DOWN</autogen>
		<autogen>__KeyToKey__ KeyCode::K, ModifierFlag::FN|ModifierFlag::NONE, KeyCode::CURSOR_UP</autogen>
		<autogen>__KeyToKey__ KeyCode::L, ModifierFlag::FN|ModifierFlag::NONE, KeyCode::CURSOR_RIGHT</autogen>
	</item>
	<item>
		<name>ME: smart tabs browsing</name>
		<item>
			<name>Google chrome</name>
			<identifier>private.smart_tabs_browsing_chrome</identifier>
			<only>GOOGLE_CHROME</only>
			<autogen>
				__KeyToKey__ 
				KeyCode::H, ModifierFlag::COMMAND_L|ModifierFlag::FN, KeyCode::CURSOR_LEFT, ModifierFlag::COMMAND_L|ModifierFlag::OPTION_L
			</autogen>
			<autogen>
				__KeyToKey__
				KeyCode::L, ModifierFlag::COMMAND_L|ModifierFlag::FN, KeyCode::CURSOR_RIGHT, ModifierFlag::COMMAND_L|ModifierFlag::OPTION_L
			</autogen>
			<autogen>
				__KeyToKey__
				KeyCode::J, ModifierFlag::COMMAND_L|ModifierFlag::FN, KeyCode::KEY_1, ModifierFlag::COMMAND_L
			</autogen>
			<autogen>
				__KeyToKey__
				KeyCode::K, ModifierFlag::COMMAND_L|ModifierFlag::FN, KeyCode::KEY_9, ModifierFlag::COMMAND_L
			</autogen>
			<autogen>
				__KeyToKey__
				KeyCode::T, ModifierFlag::CONTROL_L|ModifierFlag::NONE, KeyCode::T, ModifierFlag::COMMAND_L|ModifierFlag::NONE
			</autogen>
			<autogen>
				__KeyToKey__
				KeyCode::W, ModifierFlag::CONTROL_L|ModifierFlag::NONE, KeyCode::W, ModifierFlag::COMMAND_L|ModifierFlag::NONE
			</autogen>
			<!-- ctrl+r to cmd+r -->
			<autogen>
				__KeyToKey__
				KeyCode::R, ModifierFlag::CONTROL_L|ModifierFlag::NONE, KeyCode::R, ModifierFlag::COMMAND_L|ModifierFlag::NONE
			</autogen>
		</item>
