	<item>
		<name>ME: Shift Delete to Forward Delete</name>
		<identifier>private.delete2forwarddelete</identifier>
		<autogen>
			__KeyToKey__ KeyCode::DELETE, MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_SHIFT | ModifierFlag::NONE,
			KeyCode::FORWARD_DELETE
		</autogen>
	</item>
	<item>
		<name>ME: disable Left/Down/Up/Right arrows</name>
		<identifier>private.disable_cursor_arrows</identifier>
		<autogen>__KeyToKey__ KeyCode::CURSOR_LEFT, KeyCode::VK_NONE</autogen>
		<autogen>__KeyToKey__ KeyCode::CURSOR_RIGHT, KeyCode::VK_NONE</autogen>
		<autogen>__KeyToKey__ KeyCode::CURSOR_UP, KeyCode::VK_NONE</autogen>
